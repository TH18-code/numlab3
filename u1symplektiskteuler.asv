h = 0.000011
N = 5000000


a = 0.5; 

q1_0 = 1 - a;
q2_0 = 0;
p1_0 = 0;
p2_0 = ((1+a)/(1-a))^0.5; 

nabla_p = @(p,q) [p(1), p(2)]'
nabla_q = @(p,q) [q(1), q(2)]'./(norm(q)^3)


q = [q1_0, q2_0]';
p = [p1_0, p2_0]';
A = [q];

for i = 1:1:N
    p = p - nabla_q(p,q).*h;
% notera att nu Ã¤r p uppdaterad fast det spelar ingen roll egentligen
    q = q + nabla_p(p,q).*h; 

    A(:, i + 1) = q;
end



plot_columns(A);

